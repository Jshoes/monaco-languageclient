{
  "name": "monaco-languageclient",
  "version": "0.19.0-next.1",
  "description": "Monaco Language client implementation",
  "author": {
    "name": "TypeFox GmbH ",
    "url": "http://www.typefox.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeFox/monaco-languageclient.git"
  },
  "bugs": {
    "url": "https://github.com/TypeFox/monaco-languageclient/issues"
  },
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/cjs/index.js",
      "types": "./lib/cjs/index.d.ts"
    },
    "./bundle-cjs": {
      "default": "./dist/mlc-cjs.js",
      "types": "./lib/cjs/index.d.ts"
    },
    "./lib-es": {
      "import": "./lib/es/index.js",
      "types": "./lib/es/index.d.ts"
    },
    "./bundle-es": {
      "import": "./dist/mlc-es.js",
      "types": "./lib/es/index.d.ts"
    },
    "./lib-amd": {
      "default": "./lib/amd/index.js",
      "types": "./lib/amd/index.d.ts"
    },
    "./vscode-compatibility": {
      "default": "./lib/vscode-compatibility.js",
      "types": "./lib/vscode-compatibility.d.ts"
    },
    "./amd/vscode-compatibility": {
      "default": "./dist/amd/vscode-compatibility.js",
      "types": "./dist/amd/vscode-compatibility.d.ts"
    }
  },
  "files": [
    "dist",
    "lib/cjs",
    "lib/es",
    "lib/amd",
    "src"
  ],
  "dependencies": {
    "glob-to-regexp": "^0.4.1",
    "vscode-jsonrpc": "6.0.0",
    "vscode-languageclient": "7.0.0",
    "vscode-languageserver-textdocument": "1.0.4",
    "vscode-uri": "3.0.3"
  },
  "devDependencies": {
    "@types/vscode": "1.66.0",
    "@types/node": "^16.11.7",
    "@types/glob-to-regexp": "^0.4.1"
  },
  "scripts": {
    "clean": "npx shx rm -fr lib dist tsconfig.tsbuildinfo",
    "compile": "tsc -p tsconfig.cjs.json",
    "watch": "tsc -w -p tsconfig.cjs.json",
    "compile:es": "tsc -p tsconfig.es.json",
    "compile:amd": "tsc -p tsconfig.amd.json",
    "build:libs": "npm run build:amd && npm run build:umd && npm run build:es",
    "build:bundles": "vite --config vite.lib.config.ts build",
    "build": "npm run clean && npm run compile && npm run compile:es && npm run compile:amd && npm run build:bundles"
  }
}
